# Development environment variables

# Database
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=app_db_dev
POSTGRES_PORT=5432

# Redis
REDIS_PORT=6379

# API
API_PORT=8000

# X (Twitter) API
X_CLIENT_ID=your_dev_client_id_here
X_CLIENT_SECRET=your_dev_client_secret_here
X_ACCESS_TOKEN=your_access_token
X_ACCESS_TOKEN_SECRET=your_access_token_secret

X_API_TIER=free
DRY_RUN=true

# Environment
ENVIRONMENT=dev
LOG_LEVEL=debug

# Timezone
DEFAULT_TIMEZONE=UTC
